{
    "componentPath": "./src/wwElement.vue",
    "editor": {
        "label": {
            "en": "reCAPTCHA"
        },
        "styleOptions": {
            "theme": {
                "label": {
                    "en": "Theme",
                    "fr": "Theme"
                },
                "type": "TextSelect",
                "options": {
                    "options": [
                        {
                            "value": "light",
                            "label": { "en": "Light" }
                        },
                        { "value": "dark", "label": { "en": "Dark" } }
                    ]
                }
            },
            "size": {
                "label": {
                    "en": "Size",
                    "fr": "Size"
                },
                "type": "TextSelect",
                "options": {
                    "options": [
                        {
                            "value": "normal",
                            "label": { "en": "Normal" }
                        },
                        { "value": "compact", "label": { "en": "Compact" } }
                    ]
                }
            }
        },
        "settingsOptions": {
            "name": {
                "label": {
                    "en": "Name"
                },
                "options": {
                    "placeholder": "Name"
                },
                "type": "Text",
                "bindable": true
            },
            "key": {
                "label": {
                    "en": "API Key"
                },
                "options": {
                    "placeholder": "key"
                },
                "type": "Text",
                "bindable": true
            },
            "sendResponse": {
                "label": {
                    "en": "Send value at form submit"
                },
                "type": "OnOff"
            }
